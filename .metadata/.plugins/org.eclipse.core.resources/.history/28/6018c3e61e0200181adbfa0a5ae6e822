package Test;

public class InsertationArray {
	
  int [] arrayToSort = {5,6,7,9,3,2,1,4};
  int arraySize = 8;
	
  public void insertationSort() {
	  
	for (int i = 1; i< arraySize; i++) {
		
		int j=1;
		int key = arrayToSort[i];
		while ((j > 0) && arrayToSort[j-1] >key) {
			
			arrayToSort[j] = arrayToSort[j-1];
			j--;
			
		}
		arrayToSort[j] = key;
		
		
	}  
	  
	  
  }
  
}
